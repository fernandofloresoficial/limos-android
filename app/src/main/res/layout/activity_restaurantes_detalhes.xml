<?xml version="1.0" encoding="utf-8"?><android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:fresco="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    tools:context="br.com.limosapp.limos.RestaurantesDetalhesActivity">    <TextView        android:id="@+id/txtEndereco"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:layout_marginEnd="8dp"        android:layout_marginStart="24dp"        android:layout_marginTop="8dp"        android:fontFamily="sans-serif"        android:text="Endereço"        android:textColor="@color/cinzaescuro"        android:textSize="14sp"        android:textStyle="bold"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/ratingBar"        tools:ignore="HardcodedText" />    <ScrollView        android:id="@+id/scrollView2"        android:layout_width="match_parent"        android:layout_height="0dp"        android:layout_marginStart="8dp"        android:layout_marginTop="8dp"        android:layout_marginEnd="8dp"        android:layout_marginBottom="8dp"        app:layout_constraintBottom_toTopOf="@+id/txtInformacoes"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/txtEndereco">        <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:orientation="vertical">            <TextView                android:id="@+id/txtApresentacao"                android:layout_width="match_parent"                android:layout_height="match_parent"                android:layout_marginStart="24dp"                android:layout_marginTop="8dp"                android:layout_marginEnd="24dp"                android:fontFamily="@font/ubuntu"                android:text="Hambúrgueres artesanais feitos com todo o capricho e dedicação, pães caseiros e carnes nobres fazem do nosso hambúrguer inconfundível."                android:textAlignment="center"                android:textSize="14sp"                fresco:layout_constraintEnd_toEndOf="parent"                fresco:layout_constraintHorizontal_bias="0.0"                fresco:layout_constraintStart_toStartOf="parent"                tools:ignore="HardcodedText" />        </LinearLayout>    </ScrollView>    <com.facebook.drawee.view.SimpleDraweeView        android:id="@+id/imgFotoCapa"        android:layout_width="0dp"        android:layout_height="200dp"        android:scaleType="centerCrop"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <com.facebook.drawee.view.SimpleDraweeView        android:id="@+id/imgFotoPerfil"        android:layout_width="80dp"        android:layout_height="80dp"        android:layout_marginBottom="6dp"        android:adjustViewBounds="false"        android:scaleType="centerCrop"        app:layout_constraintBottom_toBottomOf="@+id/imgFotoCapa"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        fresco:roundAsCircle="true"        fresco:roundingBorderColor="@color/branco"        fresco:roundingBorderWidth="2dp" />    <Button        android:id="@+id/btnIncluirObservacoes"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:layout_marginBottom="24dp"        android:layout_marginEnd="16dp"        android:layout_marginStart="16dp"        android:background="@drawable/botaologin"        android:text="@string/vercardapio"        android:textColor="@android:color/white"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent" />    <TextView        android:id="@+id/txtFantasia"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:layout_marginStart="24dp"        android:layout_marginTop="16dp"        android:layout_marginEnd="16dp"        android:ellipsize="end"        android:fontFamily="@font/baloo_da"        android:singleLine="true"        android:text="The Cleaver"        android:textColor="@color/vermelholimos"        android:textSize="36sp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/imgFotoCapa"        tools:ignore="HardcodedText" />    <TextView        android:id="@+id/txtInformacoes"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="8dp"        android:layout_marginTop="24dp"        android:layout_marginEnd="8dp"        android:layout_marginBottom="32dp"        android:fontFamily="@font/architects_daughter"        android:text="@string/maisinformacoes"        android:textColor="@color/cinzaescuro"        android:textSize="24sp"        app:layout_constraintBottom_toTopOf="@+id/btnIncluirObservacoes"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/scrollView2" />    <RatingBar        android:id="@+id/ratingBar"        style="@android:style/Widget.DeviceDefault.Light.RatingBar.Small"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="24dp"        android:progressTint="@color/vermelholimos"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/txtFantasia"        tools:ignore="UnusedAttribute" />    <TextView        android:id="@+id/txtVerAvaliacoes"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="8dp"        android:layout_marginTop="2dp"        android:fontFamily="sans-serif"        android:text="@string/veravaliacoes"        android:textSize="10sp"        app:layout_constraintStart_toEndOf="@+id/ratingBar"        app:layout_constraintTop_toBottomOf="@+id/txtFantasia"        tools:ignore="SmallSp" />    <ImageView        android:id="@+id/imgVoltar"        android:layout_width="24dp"        android:layout_height="24dp"        android:layout_marginStart="8dp"        android:layout_marginTop="14dp"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        fresco:srcCompat="@drawable/ic_voltar"        tools:ignore="ContentDescription" /></android.support.constraint.ConstraintLayout>